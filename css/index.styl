$blue = #0172E6
$red = #CD3E20
$gray1 = #F3F3F3
$gray2 = #E5E5E5
$gray3 = #CFCFCF
$gray4 = #505050
$black = #131313

@import 'css/jquery-ui-1.8.22.custom.css'
@import './mixin'
@import './drop'
@import './progress'
@import './fonts'
@import './image'
@import './table'
@import './d3'

body
  margin 0 auto
  background-color $gray1
  color $gray3
  font-family 'Open Sans', sans-serif

.tab
  font-size 12px

table
  font-size 11px
  
table.header
  float left
  
  .key
    text-align right
  .value
    text-align left
  .even
    background-color #FAFAFA
  .odd
    background-color #F7F7F7

.hdu
  overflow auto

.dataunit
  float left

.plot
  float left

.column-names
  font-weight 700

div.axes
  float left
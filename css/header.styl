#header
  position fixed
  top 25%
  left 45%
  z-index 2

table.header
  font-size 11px
  color $black
  width 300px
  border 0
  overflow scroll
  
  tbody
    overflow auto
    display block
    width 100%
    
    tr
      background-color #FAFAFA
      width 100%
      height 16px
      
      &:hover
        background $gray1
        
      td:first-child
        width 100px
        font-weight 700
        text-align right
        padding-right 4px
        
      td:last-child
        width 200px
        text-align left
        padding-left 4px
        
    tr:nth-child(2n+1)
      background-color #F7F7F7
      
        
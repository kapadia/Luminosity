canvas
  padding 0
  margin 0

.dataunit-item
  float left

.chart rect
  stroke white
  fill $red
  opacity 1
  
  .selected
    fill $blue
  
.brush .extent
  fill $blue
  stroke #fff
  fill-opacity 0.125
  shape-rendering crispEdges

.fits-viewer
  border 1px solid $red
  width 600px
  height 600px
  cursor crosshair
  
  canvas
    position absolute

progress
  .read-image
    width 200px
    height 20px

.spinner
  position absolute
  top 316px
  left 500px
<div id='header' class='modal'></div>
<div id='info'></div>

<% for hdu, index in @: %>
<input type='radio' name='hdu' id='hdu<%- index %>'>
<% end %>

<div id="slides">
  <div id="overflow">
    <div class="inner">
      <% for hdu, index in @: %>
        <article>
          <p>DataUnit <%- index %></p>
        </article>
      <% end %>
    </div>
  </div>
</div>

<div id="active">
<% for hdu, index in @: %>
  <label for='hdu<%- index %>'></label>
<% end %>
</div>